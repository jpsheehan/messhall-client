{
    "extends": ["google", "plugin:react/recommended"],
    "parserOptions": {
        "ecmaVersion": 2018,
        "sourceType": "module"
    },
    "ecmaFeatures": {
        "jsx": true
    },
    "env": {
        "node": true
    },
    "rules":{
        "padded-blocks": ["error", {"blocks": "always"}],
        "linebreak-style":"off"
    }

}